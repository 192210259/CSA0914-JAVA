public class productofdigits {
    public static void main(String[] main)
    {
        int num=123;
        int product=1;
        while(num>0)
        {
            int digit =num%10;
            if(digit==0)
            {
                product=0;
                break;
            }
            product*=digit;
            num/=10;
        }
        System.out.println("product of digits:"+product);
    }
}
